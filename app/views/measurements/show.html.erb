<div class="panel panel-default">

 <div class="panel panel-info">
 <div class="panel-heading">Измерение</div>
 <div class="panel-body" style="">

    <table class="table table-striped table-condensed table-bordered table-hover">
  <tr>
    <th>File name</th>
    <th>Count</th>
    <th>Bin Time</th>
    <th>T</th>
    <th>C</th>
    <th>n</th>
    <th>x</th>
    <th>y</th>
    <th>z</th>
    <th>std</th>
  </tr>


    <tr>
      <td><%= @measurement.fileName %></td>
      <td><%= @measurement.count %></td>
      <td><%= @measurement.binTime %></td>
      <td><%= @measurement.T %></td>
      <td><%= @measurement.C %></td>
      <td><%= @measurement.n %></td>
      <td><%= @measurement.x %></td>
      <td><%= @measurement.y %></td>
      <td><%= @measurement.z %></td>
      <td><%= @measurement.std %></td>
   </tr>

</table>
<div style="float:left;">
  <%= link_to 'Назад', measurements_path, :class => 'btn btn-default btn-md' %>
  <%= link_to 'Изменить', edit_measurement_path(@measurement), :class => 'btn btn-default btn-md' %>
 </div>
