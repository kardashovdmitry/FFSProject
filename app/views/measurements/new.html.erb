<h1>Добавить измерение</h1>
<div class="col-md-4 col-md-offset-4">

<div class="row" style="margin-bottom: 40px; margin-top: 20px">
      <div class="col-xs-6">
        <%= form_tag parse_attachments_path, :multipart => true do %>
           <label for="file">Загрузить данные</label> <%= file_field_tag "file"%>
           <%= submit_tag("Загрузить данные из файла", :class => "btn btn-info" )%>
        <% end %>
      </div>
      <div class="col-xs-6">
        <%= form_tag display_graphics_attachments_path, :multipart => true do %>
           <label for="file">Построить графики</label> <%= file_field_tag "file"%>
           <%= submit_tag("Построить графики", :class => "btn btn-info" )  %>
        <% end %>
      </div>
    </div>

    <%= render 'form' %>
    <div class="row" style="margin-top: 40px"></div>




</div>

